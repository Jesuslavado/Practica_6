<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/Android"
        android:layout_width="439dp"
        android:layout_height="442dp"
        android:layout_marginBottom="380dp"
        android:src="@tools:sample/avatars"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/background_1" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/Estrella"
        android:layout_width="55dp"
        android:layout_height="68dp"
        android:layout_marginStart="340dp"
        android:layout_marginBottom="436dp"
        android:backgroundTint="#FB9EE0"
        android:clickable="true"
        android:contentDescription="Favoritos"

        app:tint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_star"
        />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="56dp"
        android:layout_height="58dp"
        android:layout_marginTop="292dp"
        android:layout_marginEnd="336dp"
        android:src="@tools:sample/avatars"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_phone" />

    <TextView
        android:id="@+id/Movil_1"
        android:layout_width="101dp"
        android:layout_height="29dp"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="4dp"
        android:text="687100200"
        app:layout_constraintBottom_toTopOf="@+id/Movil1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/Movil1"
        android:layout_width="98dp"
        android:layout_height="34dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="360dp"
        android:text="Movil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="83dp"
        android:layout_height="46dp"
        android:layout_marginStart="96dp"
        android:layout_marginBottom="376dp"
        android:src="@tools:sample/avatars"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Movil1"
        app:srcCompat="@drawable/ic_sms"
        app:tint="#009688" />

    <TextView
        android:id="@+id/Movil2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:text="699500600"
        app:layout_constraintBottom_toTopOf="@+id/Movil_2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/Movil_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginBottom="304dp"
        android:text="Movil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="83dp"
        android:layout_height="46dp"
        android:layout_marginStart="124dp"
        android:layout_marginBottom="296dp"
        android:src="@tools:sample/avatars"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Movil2"
        app:srcCompat="@drawable/ic_sms"
        app:tint="#009688" />

    <View
        android:id="@+id/divider2"
        android:layout_width="320dp"
        android:layout_height="1dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="276dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="63dp"
        android:layout_height="47dp"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="209dp"
        android:src="@tools:sample/avatars"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_email" />

    <TextView
        android:id="@+id/gmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:text="info@gmail.com"
        app:layout_constraintBottom_toTopOf="@+id/mail"
        app:layout_constraintStart_toEndOf="@+id/imageView5" />

    <TextView
        android:id="@+id/mail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginBottom="209dp"
        android:text="Mail"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView5" />

</androidx.constraintlayout.widget.ConstraintLayout>